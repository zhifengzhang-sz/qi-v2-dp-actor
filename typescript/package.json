{
  "name": "@qi/dp-actor",
  "version": "ts-0.7.0-dev",
  "description": "Data Processing Actors - Market data DSL and actor system",
  "type": "module",
  "workspaces": [
    "lib/*",
    "../../qi-v2-qicore/typescript/lib/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dsl": {
      "types": "./lib/dsl/dist/index.d.ts",
      "import": "./lib/dsl/dist/index.js",
      "require": "./lib/dsl/dist/index.cjs"
    },
    "./md": {
      "types": "./lib/md/dist/index.d.ts",
      "import": "./lib/md/dist/index.js",
      "require": "./lib/md/dist/index.cjs"
    },
    "./actor": {
      "types": "./lib/actor/dist/index.d.ts",
      "import": "./lib/actor/dist/index.js",
      "require": "./lib/actor/dist/index.cjs"
    },
    "./connectors": {
      "types": "./lib/connectors/dist/index.d.ts",
      "import": "./lib/connectors/dist/index.js",
      "require": "./lib/connectors/dist/index.cjs"
    },
    "./utils": {
      "types": "./lib/utils/dist/index.d.ts",
      "import": "./lib/utils/dist/index.js",
      "require": "./lib/utils/dist/index.cjs"
    },
    "./base": {
      "types": "./lib/base/dist/index.d.ts",
      "import": "./lib/base/dist/index.js",
      "require": "./lib/base/dist/index.cjs"
    },
    "./generic": {
      "types": "./lib/generic/dist/index.d.ts",
      "import": "./lib/generic/dist/index.js",
      "require": "./lib/generic/dist/index.cjs"
    },
    "./mcp": {
      "types": "./lib/mcp/dist/index.d.ts",
      "import": "./lib/mcp/dist/index.js",
      "require": "./lib/mcp/dist/index.cjs"
    }
  },
  "bin": {
    "qi-mcp-server": "./dist/bin/mcp-server-simple.js"
  },
  "scripts": {
    "dev": "bun run lib/src/index.ts",
    "build": "bun run build:modules && tsup lib/src/index.ts --format cjs,esm --dts --clean",
    "build:modules": "bun run build:dsl && bun run build:md && bun run build:actor && bun run build:base && bun run build:generic && bun run build:mcp && bun run build:connectors && bun run build:utils",
    "build:dsl": "cd lib/dsl && bun run build",
    "build:md": "cd lib/md && bun run build",
    "build:actor": "cd lib/actor && bun run build",
    "build:base": "cd lib/base && bun run build",
    "build:generic": "cd lib/generic && bun run build",
    "build:mcp": "cd lib/mcp && bun run build",
    "build:connectors": "cd lib/connectors && bun run build",
    "build:utils": "cd lib/utils && bun run build",
    "build:watch": "tsup lib/src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:contracts": "vitest run tests/contracts/",
    "test:e2e": "vitest run tests/e2e/",
    "bench": "vitest bench",
    "format": "biome check . --write",
    "format:check": "biome check .",
    "lint": "biome lint . && eslint lib/dsl/src lib/actor/src --ext .ts,.tsx",
    "lint:dsl": "eslint lib/dsl/src --ext .ts,.tsx",
    "lint:md": "eslint lib/md/src --ext .ts,.tsx",
    "lint:base": "eslint lib/base/src --ext .ts,.tsx",
    "lint:utils": "eslint lib/utils/src --ext .ts,.tsx",
    "lint:actor": "eslint lib/actor/src --ext .ts,.tsx",
    "lint:generic": "eslint lib/generic/src --ext .ts,.tsx",
    "lint:mcp": "eslint lib/mcp/src --ext .ts,.tsx",
    "lint:fix": "eslint lib/dsl/src lib/actor/src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc lib/src/index.ts --out docs/api",
    "clean": "rimraf dist docs/api && bun run clean:modules",
    "clean:modules": "cd lib/dsl && bun run clean && cd ../md && bun run clean && cd ../actor && bun run clean && cd ../connectors && bun run clean && cd ../utils && bun run clean",
    "start:mcp": "bun run dist/bin/mcp-server-simple.js",
    "dev:mcp": "bun run lib/src/bin/mcp-server-simple.ts",
    "check": "bun run typecheck && bun run format && bun run test",
    "prepublishOnly": "bun run build && bun run test && bun run format",
    "deps:check": "npm list --depth=0",
    "verify:dsl:impl": "echo 'Use Claude Code CLI: /verify-dsl-impl'",
    "verify:dsl:doc": "echo 'Use Claude Code CLI: /verify-dsl-doc'"
  },
  "keywords": [
    "qicore",
    "market-data",
    "dsl",
    "actor-system",
    "cryptocurrency",
    "fix-protocol",
    "streaming",
    "mcp",
    "financial-data",
    "typescript"
  ],
  "author": "Qi Platform",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qi-platform/qi-v2-dp-actor.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/qi-platform/qi-v2-dp-actor/issues"
  },
  "homepage": "https://github.com/qi-platform/qi-v2-dp-actor/tree/main/typescript",
  "files": ["dist", "README.md", "CHANGELOG.md"],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@confluentinc/kafka-javascript": "^1.4.1",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@qi/base": "workspace:../../qi-v2-qicore/typescript/lib/base",
    "@qi/core": "workspace:../../qi-v2-qicore/typescript/lib/core",
    "@vitest/coverage-v8": "1.6.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "decimal.js": "^10.6.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "rxjs": "^7.8.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@qi/eslint-plugin": "workspace:../../qi-v2-qicore/typescript/eslint-rules",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@types/decimal.js": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "bun-types": "^1.1.0",
    "eslint": "^9.35.0",
    "fast-check": "^3.15.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.43.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "optionalDependencies": {
    "@timescale/toolkit": "^1.0.0",
    "pg": "^8.11.3",
    "@types/pg": "^8.10.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/qi-platform"
  }
}
